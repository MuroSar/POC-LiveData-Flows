<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.poclivedataflows">

    <application
        android:name="com.example.poclivedataflows.utils.PocLiveDataFlowsApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PocLiveDataFlows">
        <activity
            android:name="com.example.poclivedataflows.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.PocLiveDataFlows.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
